<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{active}}"  bind:change="onChange">
  <van-tabbar-item wx:for="{{list}}" wx:key="index" icon-size="20">
    <image 
      slot="icon" 
      src="{{item.iconPath}}" 
      mode="aspectFit" 
      style="width:20px; height:20px; display: block;"
    ></image>
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>