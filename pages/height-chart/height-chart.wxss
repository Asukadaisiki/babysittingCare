
.container {
    padding: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  
  .chart-title {
    font-size: 36rpx;
    font-weight: bold;
    margin-bottom: 20rpx;
  }
  
  .f2-chart {
      width: 100%;
      height: 500rpx;
    }
    
  .chart-container {
    width: 100%;
    height: 300px;
    margin: 10px 0;
  }
  
  .chart-info {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 30rpx;
  }
  
  .chart-info text {
    margin: 10rpx 0;
    color: #666;
  }
  
  .back-button {
    width: 80%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background-color: #1890FF;
    color: white;
    border-radius: 10rpx;
    margin-top: 20rpx;
  }